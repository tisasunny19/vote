void sumcheck(int num)
{int i,j,sum;
for(i=2;i<(num/2);i++)
    {  if(primecheck(i)==1)
     for(j=(num/2);j<=num;j++)
        { if(primecheck(j)==1)
             { if((i+j)==num)
              cout<<num<<"="<<i<<"+"<<j<<"\n";
             }
          }   
     }
}
int main(){ int num;
cout<<"\nEnter a Positvie Number : ";
cin>>num;
